
:START
WAITCALL LS=CALL LT=START
GOTO START

:CALL
PU
PLAY HELLO.WAW

:Q1
QUESTION QUEST.WAW L1=RECMSG L2=LENA L3=FREDRIK L9=CDE LT=ENDIT LH=ENDIT
PLAY CORRECT.WAW
GOTO Q1

:RECMSG
PLAY TONE.WAW
RECMSG LD=Q1 LH=ENDIT

:LENA
PLAY LENA.WAW
GOTO Q1

:FREDRIK
PLAY FREDRIK.WAW
GOTO Q1

:CDE
ENTERCODE CODE.WAW 0321 LC=Q2 LI=WRONG LT=ENDIT LH=ENDIT
GOTO CDE

:WRONG
PLAY NOOK.WAW
GOTO Q1

:Q2
QUESTION OPQUEST.WAW L1=ANSON L2=ANSOFF L3=PLYNEW L9=ERSALL LT=ENDIT LH=ENDIT
GOTO Q2

:ANSON
ANSON
GOTO Q2

:ANSOFF
ANSOFF
GOTO Q2

:PLYNEW
PLAYNEW
GOTO Q2

:ERSALL
ERASEDYNAMIC
GOTO Q2


:ENDIT
PD
GOTO START

